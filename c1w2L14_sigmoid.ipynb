   "source": [
    "import matplotlib.pyplot as plt \n",
    "import numpy as np \n",
    "\n",
    "z = np.linspace(-10, 10, 100) \n",
    "def sigmoid(z):\n",
    "    return 1/(1 + np.exp(-z))\n",
    "\n",
    "a = sigmoid(z)\n",
    "plt.plot(z, a) \n",
    "plt.xlabel(\"z\") \n",
    "plt.ylabel(\"sigmoid(z)\")"
   ]
  },
